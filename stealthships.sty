\tikzset{onestar/.pic={
\node[draw=black, circle, inner sep=0.75pt, line width=1.25mm] at (0,0)  {\normalsize\phantom{1}};
\node[draw=lightgray, circle, inner sep=0.75pt, line width=0.9mm] at (0,0)  {\normalsize\phantom{1}};
}}

\tikzset{twostar/.pic={
\node[text=black, rotate=45, inner sep=0pt] at (0,0) {\rule{5.85mm}{1.35mm}};
\node[text=black, rotate=-45, inner sep=0pt] at (0,0) {\rule{5.85mm}{1.35mm}};

\node[text=lightgray, rotate=45, inner sep=0pt] at (0,0) {\rule{5.5mm}{1mm}};
\node[text=lightgray, rotate=-45, inner sep=0pt] at (0,0) {\rule{5.5mm}{1mm}};
}}

\tikzset{threestar/.pic={
\node[inner sep=0pt] at (0,0) {};
\node[regular polygon, regular polygon sides=3, draw=gray, line width=1.25mm, inner sep=0.775pt] at (0,-0.7mm) {\normalsize\phantom{.}};
\node[regular polygon, regular polygon sides=3, draw=white, line width=0.9mm, inner sep=0.75pt] at (0,-0.7mm) {\normalsize\phantom{.}};
}}

\tikzset{fourstar/.pic={
\node[draw=gray, diamond, inner sep=2.225pt, line width=1.25mm] at (0,0) {\normalsize\phantom{.}};
\node[draw=white, diamond, inner sep=2.125pt, line width=0.9mm] at (0,0) {\normalsize\phantom{.}};
}}


\newcommand{\drawonestar}{\scalebox{0.75}{\tikz{\pic {onestar};}}}
\newcommand{\drawtwostar}{\scalebox{0.75}{\tikz{\pic {twostar};}}}
\newcommand{\drawthreestar}{\raisebox{0.0ex}{\scalebox{0.75}{\tikz{\pic {threestar};}}}}
\newcommand{\drawfourstar}{\scalebox{0.75}{\tikz{\pic {fourstar};}}}
